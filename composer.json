{
    "name": "chadicus/psr-log-newrelic",
    "description": "PSR-3 Log Implementation with NewRelic",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0",
        "psr/log": "^1.0",
        "sobanvuex/php-newrelic": "^2.0",
        "chadicus/psr-log-helper": "^1.2"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Chadicus\\Psr\\Log\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChadicusTest\\Psr\\Log\\": "src"
        }
    }
}
