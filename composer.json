{
    "name": "subjective-php/psr-log-newrelic",
    "description": "PSR-3 Log Implementation with NewRelic",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
   "provide": {
        "psr/log-implementation": "1.0"
    },
    "require": {
        "php": "^7.0||^8.0",
        "psr/log": "^1.0",
        "sobanvuex/php-newrelic": "^2.0",
        "subjective-php/psr-log-helper": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.5",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "SubjectivePHP\\Psr\\Log\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SubjectivePHPTest\\Psr\\Log\\": "src"
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs",
        "test": "vendor/bin/phpunit"
    }
}
